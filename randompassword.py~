#!usr/bin/python3

import secrets
import string

letters = string.ascii_letters
digits = string.digits
alphabet = letters + digits

pwd_l = int(input("Password length?"))
if type(pwd_l) is int || if pwd_l < 8:
    pwd_l = 8
elif pwd_l > 12:
    pwd_l = 12
else:
    pwd_l = 8
    
pwd = ""
for i in range(pwd_l):
    pwd += "".join(secrets.choice(alphabet))

print(pwd)
